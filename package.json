{
  "name": "server-side-express",
  "version": "0.0.1",
  "description": "Server side rendering",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=development node_modules/.bin/jest",
    "start": "",
    "dev": "DEBUG=server-render/* node_modules/.bin/nodemon ./bin/www",
    "build:2:watch": "rm -rf dist/ && babel --watch src/ -d build/",
    "build:2": "rm -rf build/ && babel src/ -d build/ && webpack -p",
    "webpack:server": "node_modules/.bin/webpack",
    "build": "NODE_ENV=development node_modules/.bin/webpack --config webpack.config.babel.js",
    "start:server": "node_modules/.bin/nodemon --inspect server",
    "dev:server": "NODE_ENV=development TARGET_ENV=server npm run webpack:server -- --watch --debug & NODE_ENV=development npm run start:server",
    "dev:client": "NODE_ENV=development npm run start:client",
    "start:client": "NODE_ENV=development node_modules/.bin/webpack-dev-server --port=3000 --inline --hot",
    "storybook": "node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "lint": "node_modules/.bin/eslint \"@(src|server|config|test)/**/*.@(js|jsx)\""
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/__mocks__/setup.js"
    ],
    "transform": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/style-mock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src",
      "server"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "globals": {
      "__DEV__": true,
      "__WEB_ROOT__": "/"
    },
    "roots": [
      "<rootDir>"
    ],
    "projects": [
      "<rootDir>"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanluke/ssr-rendering.git"
  },
  "keywords": [
    "SSR"
  ],
  "author": "Lukasz Gonciarz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanluke/ssr-rendering/issues"
  },
  "homepage": "https://github.com/vanluke/ssr-rendering#readme",
  "dependencies": {
    "@storybook/react": "^3.2.8",
    "body-parser": "^1.18.2",
    "convict": "^4.0.0",
    "express": "^4.15.4",
    "font-awesome": "^4.7.0",
    "mori": "^0.3.2",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.1.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.16.0",
    "redux0-helpers": "^1.0.0",
    "rx-http-request": "^1.2.0",
    "rxjs": "^5.4.3",
    "styled-components": "^2.1.2",
    "url": "^0.11.0",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-refresh": "^1.7.2",
    "browser-sync": "^2.18.13",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "express-handlebars": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "isomorphic-style-loader": "^4.0.0",
    "jest": "^21.1.0",
    "jest-cli": "^21.1.0",
    "json-loader": "^0.5.7",
    "morgan": "^1.8.2",
    "nconf": "^0.8.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.6",
    "simple-webpack-clean-plugin": "^0.1.0",
    "sinon": "^3.3.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  }
}
